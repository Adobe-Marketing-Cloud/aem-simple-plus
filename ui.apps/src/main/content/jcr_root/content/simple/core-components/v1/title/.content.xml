<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/simple/settings/wcm/templates/content-page"
        jcr:primaryType="cq:PageContent"
        jcr:title="Title Component (v1)"
        sling:resourceType="simple/components/structure/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <text-h1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="simple/components/content/text"/>
                <title-h2
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title component set to H1"
                    sling:resourceType="simple/components/core/v1/title"/>
                <title-h2
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title component set to H2"
                    sling:resourceType="simple/components/core/v1/title"
                    type="h2"/>
                <title-h3
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title component set to H3"
                    sling:resourceType="simple/components/core/v1/title"
                    type="h3"/>
                <title-h4
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title component set to H4"
                    sling:resourceType="simple/components/core/v1/title"
                    type="h4"/>
                <title-h5
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title component set to H5"
                    sling:resourceType="simple/components/core/v1/title"
                    type="h5"/>
            </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>
